package comandos;

import java.util.HashMap;
import java.util.Map;


/*Dada a população estimada de alguns estados do NE brasileiro, faça:
 * Estado = PE - População = 9.616.621
 * Estado = AL - População = 93.351.543
 * Estado = CE - População = 9.187.103
 * Estado = RN - População = 3.534.265*/
public class ExerciciosMap {

	public static void main(String[] args) {
		System.out.println("CRIE UM DICIONÁRIO E RELACIONE OS ESTADOS E SUA POPULAÇÕES");
		Map<String, EstadoPopulacao> estadosPopulacoes = new HashMap<>(){{
			put("Estado", new EstadoPopulacao("PE", 9616.621));
			put("Estado", new EstadoPopulacao("AL", 3351.543));
			put("Estado", new EstadoPopulacao("CE", 9187.103));
			put("Estado", new EstadoPopulacao("RN", 3534.265));
		}
		};

		System.out.println(estadosPopulacoes.toString());

	}

}
